{
    "name": "tervis/event-logger",
    "description": "Monolog event database logger",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Mika Tervonen",
        "email": "mtervonen80@gmail.com"
      }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.0",
        "symfony/monolog-bundle": "^3.5",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "tervis\\EventLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tervis\\EventLogger\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    }
}
